{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
		"border": {
            "radius": false,
            "color": false,
            "style": false,
            "width": false
        },
		"color": {
            "custom": false,
            "customGradient": false,
			"customDuotone": false,
			"defaultDuotone": false,
            "gradients": [],
            "link": false,
            "defaultPalette": false,
            "defaultGradients": false,
			"text": true,
            "background": true,
            "palette": [
                {
                    "slug": "pink-400",
                    "color": "#d62293",
                    "name": "Pink 400"
                },
                {
                    "slug": "tertiary-400",
                    "color": "#62a1ff",
                    "name": "Tertiary 400"
                },
                {
                    "slug": "black",
                    "color": "#000000",
                    "name": "Black"
                },
                {
                    "slug": "gray-700",
                    "color": "#747474",
                    "name": "Gray 700"
                },
                {
                    "slug": "gray-500",
                    "color": "#c4c4c4",
                    "name": "Gray 500"
                },
                {
                    "slug": "gray-400",
                    "color": "#e6e6e6",
                    "name": "Gray 400"
                },
                {
                    "slug": "gray-300",
                    "color": "#eeeeee",
                    "name": "Gray 300"
                },
                {
                    "slug": "gray-200",
                    "color": "#f8f8f8",
                    "name": "Gray 200"
                },
                {
                    "slug": "gray-100",
                    "color": "#fcfcfc",
                    "name": "Gray 100"
                },
				{
                    "slug": "gray-050",
                    "color": "#fdfdfd",
                    "name": "Gray 50"
                },
                {
                    "slug": "white",
                    "color": "#FFFFFF",
                    "name": "White"
                },
				{
                    "slug": "purple",
                    "color": "#4A23CC",
                    "name": "Purple"
                }
            ]
        },
        "layout": {
            "contentSize": "1200px"
        },
		"spacing": {
			"blockGap": null,
			"margin": true,
			"padding": true,
			"units": [ "px", "rem", "vh", "vw", "%" ]
		},
        "typography": {
            "customFontSize": false,
			"lineHeight": false,
            "dropCap": false,
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": false,
            "textDecoration": false,
			"textTransform": true,
			"fontSizes": [
                {
                    "slug": "extra-small",
                    "size": "10px",
                    "name": "Extra small"
                },
                {
                    "slug": "small",
                    "size": "14px",
                    "name": "Small"
                },
                {
                    "slug": "normal",
                    "size": "16px",
                    "name": "Normal"
                },
                {
                    "slug": "medium",
                    "size": "18px",
                    "name": "Medium"
                },
                {
                    "slug": "large",
                    "size": "20px",
                    "name": "Large"
                },
                {
                    "slug": "larger",
                    "size": "24px",
                    "name": "Larger"
                },
                {
                    "slug": "extra-large",
                    "size": "36px",
                    "name": "Extra large"
                },
				{
                    "slug": "extra-extra-large",
                    "size": "46px",
                    "name": "Extra Extra large"
                }
            ],
            "fontFamilies": [
                {
                    "fontFamily": "Clarity City,-apple-system,BlinkMacSystemFont,Segoe UI,Helvetica,Arial,sans-serif,Roboto,Ubuntu,Apple Color Emoji,Segoe UI Emoji,Segoe UI Symbol",
                    "slug": "primary",
                    "name": "Primary"
                },
				{
                    "fontFamily": "Source Serif Pro, Apple Garamond, Baskerville, Times New Roman, Droid Serif, Times, Source Serif Pro, serif",
                    "slug": "serif",
                    "name": "Serif"
                }
            ]
        },
        "blocks": {
            "core/button": {
                "border": {
                    "radius": false
                },
                "color": {
					"text" : false,
                    "palette": [
                        {
                            "slug": "pink-400",
                            "color": "#d62293",
                            "name": "Pink 400"
                        },
                        {
                            "slug": "black",
                            "color": "#000000",
                            "name": "Black"
                        }
                    ]
                },
				"typography": {
					"customFontSize": false,
					"dropCap": false,
					"fontFamilies": [],
					"fontSizes": [],
					"fontStyle": false,
					"fontWeight": false,
					"letterSpacing": false,
					"lineHeight": false,
					"textDecoration": false,
					"textTransform": false
				}
            }
        }
    },
	"styles": {
		"color": {
			"text": "var(--wp--preset--color--black)",
            "background": "var(--wp--preset--color--gray-050)"
        },
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"fontWeight": "400",
			"textDecoration": "none"
		},
		"blocks": {
			"core/buttons": {
				"spacing": {
					"blockGap": "30px"
				}
			},
			"core/columns": {
				"spacing": {
					"blockGap": "39px"
				}
			},
			"core/quote": {
				"border": {
					"width": "0",
					"style": "none"
				},
				"spacing": {
					"padding": {
						"left": "0"
					},
					"margin": {
						"bottom": "50px",
						"left": "0",
						"right": "0",
						"top": "0"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serif)",
					"fontStyle": "italic",
					"lineHeight": "175%",
					"letterSpacing": "0.02em",
					"fontWeight": "400",
					"fontSize": "36px"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "18px"
						},
						"spacing": {
							"margin": {
								"top": "20px"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--gray-700)"
						}
					}
				}
			}
        },
		"elements": {
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--black)"
				},
				"typography": {
					"fontWeight": "700"
				}
			},
			"button": {
				"border": {
					"radius": "5px"
				},
				"color": {
					"background": "var(--wp--preset--color--pink-400)",
					"text": "var(--wp--preset--color--white)"
				},
				"spacing": {
					"padding": {
						"top": "11px",
						"right": "40px",
						"bottom": "11px",
						"left": "40px"
					}
				},
				"typography": {
					"fontWeight": "700",
					"lineHeight": "18px",
					"fontSize": "16px",
					"textTransform": "uppercase",
					"textDecoration": "none",
					"letterSpacing": "0.02em"
				},
				":hover": {
					"color": {
						"background": "#c53490",
						"text": "var(--wp--preset--color--white)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":visited": {
					"color": {
						"text": "var(--wp--preset--color--white)"
					},
					"typography": {
						"textDecoration": "none"
					}
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--pink-400)"
				},
				"typography": {
					"textDecoration": "none",
					"fontWeight": "700"
				}
			}
		}
	}
}