{
  "extends": [
    "eslint:recommended",
    "plugin:@wordpress/eslint-plugin/recommended"
  ],
  "env": {
    "browser": true,
    "es2020": true,
    "node": true,
    "jquery": true
  },
  "globals": {
    "jQuery": true,
    "wp": true
  },
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "prefer-const": "off" //es6
  }
}
